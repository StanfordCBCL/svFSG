# Equations
Add equation: struct {
   Coupled: 1
   Min iterations: 1
   Max iterations: 20
   Tolerance: 1e-4

   Constitutive model: mm
   #Dilational penalty model: M94
   Penalty parameter: 100000000.0
   Density: 0.0
   Mass damping: 800000.0

   LS type: BICGS {
      Preconditioner: Trilinos-BlockJacobi
      Tolerance: 0.1
      Max iterations: 1000
      Krylov space dimension: 250
   }
   
   Output: Spatial {
      Displacement: t
      Velocity: t
      Jacobian: t
      Stress: t
      Strain: t
      Cauchy_stress: t
      Def_grad: t
   }

   Add BC: wall_inlet {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   Add BC: wall_outlet {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   Add BC: wall_inner {
      Type: Neu
